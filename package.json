{
  "name": "bluedart-clone",
  "version": "1.0.0",
  "description": "This project is a basic clone of Bluedart, a courier and logistics service. It includes essential features such as booking shipments, tracking packages, managing delivery schedules, and sending notifications to customers. Additionally, it integrates AWS S3 for document storage and Nodemailer for email notifications.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adityagkmit/bluedart-clone.git"
  },
  "author": "\"Aditya Singh Rajawat\"",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adityagkmit/bluedart-clone/issues"
  },
  "homepage": "https://github.com/adityagkmit/bluedart-clone#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  }
}
